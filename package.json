{
  "name": "@muenzpraeger/sfdx-plugin",
  "description": "A plugin for the Salesforce CLI built by René Winkelmeyer.",
  "version": "0.2.0",
  "author": "René Winkelmeyer @muenzpraeger",
  "bugs": "https://github.com/muenzpraeger/sfdx-plugin/issues",
  "dependencies": {
    "@oclif/command": "^1.4.13",
    "@oclif/config": "^1.6.3",
    "@oclif/errors": "^1.0.4",
    "@salesforce/command": "^0.1.1",
    "@types/fs-extra": "^5.0.2",
    "@types/recursive-readdir": "^2.2.0",
    "fs-extra": "^6.0.0",
    "npm": "^5.8.0",
    "recursive-readdir": "^2.2.2",
    "replace-in-file": "^3.4.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.13.0",
    "@oclif/plugin-help": "^1.2.4",
    "@oclif/test": "^1.0.4",
    "@oclif/tslint": "^1.1.0",
    "@salesforce/dev-config": "^1.0.4",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.4",
    "chai": "^4.1.2",
    "globby": "^8.0.1",
    "mocha": "^5.0.5",
    "sinon": "^4.5.0",
    "ts-node": "5",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/lib",
    "/messages",
    "/resources"
  ],
  "homepage": "https://github.com/muenzpraeger/sfdx-plugin",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "muenzpraeger/sfdx-plugin",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "clean": "rm -f .oclif.manifest.json",
    "lint": "tsc -p test --noEmit && tslint -p test -t stylish",
    "postpublish": "yarn run clean",
    "posttest": "yarn run lint",
    "prepare": "yarn run build && oclif-dev manifest",
    "prepublishOnly": "yarn run build && oclif-dev manifest",
    "preversion": "yarn run clean",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
